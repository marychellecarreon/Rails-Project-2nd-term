<div class= "review-rating" data-score= "<%= review.rating %>"></div>
<p><%= review.comment %></p>

<% if user_signed_in? %>
    <% if review.user_id == current_user.id %>
         <strong><%= current_user.email %></strong></br>
         Posted <%= time_ago_in_words(review.created_at) %> ago
   
   <% end %>
 <% end %>